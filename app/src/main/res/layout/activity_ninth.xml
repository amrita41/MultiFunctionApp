<?xml version="1.0" encoding="utf-8"?>
    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        tools:context="com.example.dell.multifunctionapp.Ninth"
        android:background="@drawable/greys">


        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textMultiLine"
            android:textStyle="bold"
            android:ems="10"
            android:id="@+id/search_url_main"
            android:textSize="18sp"
            android:hint="Search or Enter URL"
            android:layout_alignParentTop="true"
            android:layout_marginTop="145dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

    <Button
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:text="Facebook"
            android:background="@android:color/holo_green_light"
            android:textStyle="bold"
            android:id="@+id/facebook"
        android:layout_alignTop="@+id/google"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

        <Button
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:text="Gmail"
            android:textStyle="bold"
            android:background="@android:color/holo_red_dark"
            android:id="@+id/gmail"
            android:layout_below="@+id/yahoo"
            android:layout_alignLeft="@+id/google"
            android:layout_alignStart="@+id/google" />

        <Button
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:text="Google"
            android:background="@android:color/holo_orange_light"
            android:textStyle="bold"
            android:id="@+id/google"
            android:layout_marginBottom="89dp"
            android:layout_alignParentBottom="true"
            android:layout_toLeftOf="@+id/facebook"
            android:layout_toStartOf="@+id/facebook" />

        <Button
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:text="Yahoo"
            android:textStyle="bold"
            android:id="@+id/yahoo"
            android:background="@android:color/holo_purple"
            android:layout_alignTop="@+id/google"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_toLeftOf="@+id/gmail"
            android:layout_toStartOf="@+id/gmail" />

        <Button
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:text="Linkedin"
            android:textStyle="bold"
            android:background="@android:color/holo_green_dark"
            android:id="@+id/linkedin"
            android:layout_below="@+id/facebook"
            android:layout_alignRight="@+id/button_search_ninth"
            android:layout_alignEnd="@+id/button_search_ninth" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="YOUTUBE"
            android:textStyle="bold"
            android:background="@android:color/holo_blue_dark"
            android:id="@+id/youtube"
            android:layout_below="@+id/yahoo"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_toLeftOf="@+id/gmail"
            android:layout_toStartOf="@+id/gmail" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="THE INTERNET CAFE"
        android:textStyle="bold|italic"
        android:textColor="#000000"
        android:textSize="35sp"
        android:id="@+id/textView6"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Back"
        android:textStyle="bold"
        android:id="@+id/button_back_ninth"
        android:layout_below="@+id/search_url_main"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Search"
        android:textStyle="bold"
        android:id="@+id/button_search_ninth"
        android:layout_below="@+id/search_url_main"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Some Shortcut Buttons:"
        android:textStyle="bold"
        android:textSize="18sp"
        android:id="@+id/textView8"
        android:layout_above="@+id/google"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


</RelativeLayout>

